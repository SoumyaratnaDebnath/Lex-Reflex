void loading()
{
	unsigned long ii,jj,cc=0;
	clrscr();
	cout<<"\n\n\n\n\n\n\n\n\n\n\n\t\t\t\t     LOADING";
	for(ii=0;ii<50000;ii++)
	  for(jj=0;jj<2000;jj++)
	  {
		 cc++;
		 cc=0;
	  }
  clrscr();

	cout<<"\n\n\n\n\n\n\n\n\n\n\n\t\t\t\t     LOADING.";
	for(ii=0;ii<50000;ii++)
	  for(jj=0;jj<2000;jj++)
	  {
		 cc++;
		 cc=0;
	  }
  clrscr();


	cout<<"\n\n\n\n\n\n\n\n\n\n\n\t\t\t\t     LOADING..";
	for(ii=0;ii<50000;ii++)
	  for(jj=0;jj<2000;jj++)
	  {
		 cc++;
		 cc=0;
	  }
  clrscr();

  cout<<"\n\n\n\n\n\n\n\n\n\n\n\t\t\t\t     LOADING...";
	for(ii=0;ii<50000;ii++)
	  for(jj=0;jj<2000;jj++)
	  {
		 cc++;
		 cc=0;
	  }
  clrscr();
  clrscr();
}

void congo(char name[25],int sss)
{
	srand(time(0));
	int n2;
	char a[25]="THE MONSTERTRUCK",
		 b[25]="THE A GAMEBLOWER",
		 c[25]="THE CASH LOVER",
		 d[25]="THE BOSS",
		 e[25]="THE MAN OF ACTION",
		 f[25]="THE LOVELY",
		 g[25]="THE MYSTRY",
		 h[25]="THE SOCIAL MEDIA STAR",
		 i[25]="THE FASTEST",
		 j[25]="THE ONE",
		 k[25]="THE DREAM",
		 l[25]="THE DIFFRENCE MAKER",
		 m[25]="THE NATION",
		 n[25]="THE GAMECHANGER",
		 o[25]="THE DAYDREAMER",
		 p[25]="THE BANG",
		 q[25]="THE PLAYER",
		 r[25]="THE SPEED",
		 s[25]="THE FEARLAND",
		 t[25]="THE CHALLENGER",
		 u[25]="THE FAMOUS ONE";

	for(int i2=0;i2<5;i2++)
	  for(int j2=0;j2<10;j2++)
		 n2=rand()%21;

		 cout<<"\n\n\n  FROM THE OFFICIAL SITE:  "<<endl;
	 cout<<"================================================================================"<<endl<<endl;
	 cout<<" CONGRATULATIONS.... You Scored "<<(sss*100)/40.0<<" % and made us bound to give you a title...";

	cout<<"\n\n\n\n   We give you the title of '";

	 switch(n2)
	 {
		 case 1: cout<<a;
		 break;
		 case 2: cout<<b;
		 break;
		 case 3: cout<<c;
		 break;
		 case 4: cout<<d;
		 break;
		 case 5: cout<<e;
		 break;
		 case 6: cout<<f;
		 break;
		 case 7: cout<<g;
		 break;
		 case 8: cout<<h;
		 break;
		 case 9: cout<<i;
		 break;
		 case 10: cout<<j;
		 break;
		 case 11: cout<<k;
		 break;
		 case 12: cout<<l;
		 break;
		 case 13: cout<<m;
		 break;
		 case 14: cout<<n;
		 break;
		 case 15: cout<<o;
		 break;
		 case 16: cout<<p;
		 break;
		 case 17: cout<<q;
		 break;
		 case 18: cout<<r;
		 break;
		 case 19: cout<<s;
		 break;
		 case 20: cout<<t;
		 break;
		 case 21: cout<<u;
		 break;
	}
	 cout<<"'";
	cout<<"\n\n\n\n                              '"<<name<<" ";
	 switch(n2)
	 {
		 case 1: cout<<a;
		 break;
		 case 2: cout<<b;
		 break;
		 case 3: cout<<c;
		 break;
		 case 4: cout<<d;
		 break;
		 case 5: cout<<e;
		 break;
		 case 6: cout<<f;
		 break;
		 case 7: cout<<g;
		 break;
		 case 8: cout<<h;
		 break;
		 case 9: cout<<i;
		 break;
		 case 10: cout<<j;
		 break;
		 case 11: cout<<k;
		 break;
		 case 12: cout<<l;
		 break;
		 case 13: cout<<m;
		 break;
		 case 14: cout<<n;
		 break;
		 case 15: cout<<o;
		 break;
		 case 16: cout<<p;
		 break;
		 case 17: cout<<q;
		 break;
		 case 18: cout<<r;
		 break;
		 case 19: cout<<s;
		 break;
		 case 20: cout<<t;
		 break;
		 case 21: cout<<u;
		 break;
	}
	cout<<"'"<<endl;
	cout<<"\n\n\n PRESS ENTER...";
	getch();
}

