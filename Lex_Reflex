void clp(unsigned long app)
{
	unsigned long iii;
	iii=app;



cout<<"                              "<<"\n\n\n LOOK AT IT CAREFULLY:\n";
cout<<"                              "<<"-------------------"<<endl;
cout<<"                              "<<"|                 |"<<endl;
cout<<"                              "<<"|                 |"<<endl;
cout<<"                              "<<"|                 |"<<endl;
cout<<"                              "<<"|      APPLE      |"<<endl;
cout<<"                              "<<"|                 |"<<endl;
cout<<"                              "<<"|                 |"<<endl;
cout<<"                              "<<"|                 |"<<endl;
cout<<"                              "<<"-------------------"<<endl;
delay(iii);
clrscr();

cout<<"                              "<<"\n\n\n LOOK AT IT CAREFULLY:\n";
cout<<"                              "<<"-------------------"<<endl;
cout<<"                              "<<"|                 |"<<endl;
cout<<"                              "<<"|                 |"<<endl;
cout<<"                              "<<"|                 |"<<endl;
cout<<"                              "<<"|                 |"<<endl;
cout<<"                              "<<"|                 |"<<endl;
