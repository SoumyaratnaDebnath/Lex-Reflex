void clrscr()
{
  system("cls");
}

int diffi(unsigned long app)
{
		 char choose;
	 int selll=1;
  int nn=0;

  clrscr();

  cout<<"================================================================================";
  cout<<"|||||||||||||||||||||||IIIIII      GAME MENU      IIIIII||||||||||||||||||||||||";
  cout<<"================================================================================"<<endl;

  cout<<"\n  SELECT DIFFICULTY                                     "<<endl;
  cout<<"--------------------------------------------------------------------------------"<<endl<<endl<<endl;
  cout<<"        { 1. EASY    }         "<<endl;
  cout<<"          2. MEDIUM           "<<endl;
  cout<<"          3. HARD             "<<endl;
	cout<<"\n\n   Use W and S to move";
	cout<<"\n   Press F to selllect";

  while(nn==0)
  {
		  choose=getch();

		  if(choose=='w' || choose=='W')
		  {
			 if(selll==1)
{
			 clrscr();
	 cout<<"================================================================================";
	 cout<<"|||||||||||||||||||||||IIIIII      GAME MENU      IIIIII||||||||||||||||||||||||";
	 cout<<"================================================================================"<<endl;

	 cout<<"\n  SELECT DIFFICULTY                     "<<endl;
	 cout<<"--------------------------------------------------------------------------------"<<endl<<endl<<endl;
  cout<<"        { 1. EASY    }         "<<endl;
  cout<<"          2. MEDIUM           "<<endl;
  cout<<"          3. HARD             "<<endl;
	cout<<"\n\n   Use W and S to move";
	 cout<<"\n   Press F to selllect";
	 selll=1;
}
		  else if(selll==2)

{
			 clrscr();
	 cout<<"================================================================================";
	 cout<<"|||||||||||||||||||||||IIIIII      GAME MENU      IIIIII||||||||||||||||||||||||";
	 cout<<"================================================================================"<<endl;

	 cout<<"\n  SELECT DIFFICULTY                     "<<endl;
	 cout<<"--------------------------------------------------------------------------------"<<endl<<endl<<endl;
  cout<<"        { 1. EASY    }         "<<endl;
  cout<<"          2. MEDIUM           "<<endl;
  cout<<"          3. HARD             "<<endl;
	cout<<"\n\n   Use W and S to move";
	 cout<<"\n   Press F to selllect";
	 selll=1;
}

		 else if(selll==3)
{
		  clrscr();
	 cout<<"================================================================================";
	 cout<<"|||||||||||||||||||||||IIIIII      GAME MENU      IIIIII||||||||||||||||||||||||";
	 cout<<"================================================================================"<<endl;

	 cout<<"\n  SELECT DIFFICULTY                     "<<endl;
	 cout<<"--------------------------------------------------------------------------------"<<endl<<endl<<endl;
  cout<<"          1. EASY              "<<endl;
  cout<<"        { 2. MEDIUM  }        "<<endl;
  cout<<"          3. HARD             "<<endl;

	cout<<"\n\n   Use W and S to move";
	 cout<<"\n   Press F to selllect";
	 selll=2;
}
		  }
		 else if(choose=='s' || choose=='S')
		  {

		 if(selll==1)
{
		  clrscr();
	 cout<<"================================================================================";
	 cout<<"|||||||||||||||||||||||IIIIII      GAME MENU      IIIIII||||||||||||||||||||||||";
	 cout<<"================================================================================"<<endl;

	 cout<<"\n  SELECT DIFFICULTY                     "<<endl;
	 cout<<"--------------------------------------------------------------------------------"<<endl<<endl<<endl;
  cout<<"          1. EASY              "<<endl;
  cout<<"        { 2. MEDIUM  }        "<<endl;
  cout<<"          3. HARD             "<<endl;
	cout<<"\n\n   Use W and S to move";
	 cout<<"\n   Press F to selllect";
	 selll=2;


}
	 else if(selll==2)
{
		  clrscr();
	 cout<<"================================================================================";
	 cout<<"|||||||||||||||||||||||IIIIII      GAME MENU      IIIIII||||||||||||||||||||||||";
	 cout<<"================================================================================"<<endl;

	 cout<<"\n  SELECT DIFFICULTY                     "<<endl;
	 cout<<"--------------------------------------------------------------------------------"<<endl<<endl<<endl;
  cout<<"          1. EASY              "<<endl;
  cout<<"          2. MEDIUM          "<<endl;
  cout<<"        { 3. HARD    }         "<<endl;

	cout<<"\n\n   Use W and S to move";
	 cout<<"\n   Press F to selllect";
	 selll=3;
}
	 else if(selll==3)
{
		  clrscr();
	 cout<<"================================================================================";
	 cout<<"|||||||||||||||||||||||IIIIII      GAME MENU      IIIIII||||||||||||||||||||||||";
	 cout<<"================================================================================"<<endl;

	 cout<<"\n  SELECT DIFFICULTY                     "<<endl;
	 cout<<"--------------------------------------------------------------------------------"<<endl<<endl<<endl;
  cout<<"          1. EASY              "<<endl;
  cout<<"          2. MEDIUM          "<<endl;
  cout<<"        { 3. HARD    }         "<<endl;

	cout<<"\n\n   Use W and S to move";
	 cout<<"\n   Press F to selllect";
	 selll=3;
}

	  }
	  else if(choose=='f')
		 nn=1;
  }

if(selll==1)
app=1;
if(selll==2)
app=2;
if(selll==3)
app=3;

return app;
}

void delay(unsigned long app)
{
	unsigned long d;
	if(app==1)
	  d=12000;
	else if(app==2)
	  d=8000;
	else
	  d=5000;
	unsigned long i,j,c=0;
	for(i=0;i<50000;i++)
	  for(j=0;j<d;j++)
	  {
		 c++;
	  }
}

int ck(char nn[25])
{


	char a[25]="ARRAYS",b[25]="APPLE",c[25]="GREAT",d[25]="PERSONALITY",e[25]="SEQUENCE",
		  f[25]="BOTANICALLY",g[25]="PERSONIFICATION",h[25]="MULTITASKING",i[25]="FACEBOOK",
		  j[25]="SNAPSHOTS",k[25]="METALLIZATION",l[25]="POSSIBILITY",m[25]="EXAMINATION",n[25]="PROJECTILE",o[25]="CONSTELLATION",p[25]="CONFIGURATION",
		  q[25]="ROBOTICS",r[25]="EXTINGUISHER",s[25]="NOTIFICATION",t[25]="HERITAGE",z[25]="SUBMIT";


	if(strcmpi(nn,a)==0)
	return 1;
	else
	if(strcmpi(nn,b)==0)
	return 1;
	else
	if(strcmpi(nn,c)==0)
	return 1;
	else
	if(strcmpi(nn,d)==0)
	return 2;
	else
	if(strcmpi(nn,e)==0)
	return 2;
	else
	if(strcmpi(nn,f)==0)
	return 2;
	else
	if(strcmpi(nn,g)==0)
	return 2;
	else
	if(strcmpi(nn,h)==0)
	return 3;
	else
	if(strcmpi(nn,i)==0)
	return 3;
	else
	if(strcmpi(nn,j)==0)
	return 3;
	else
	if(strcmpi(nn,k)==0)
	return 3;
	else
	if(strcmpi(nn,l)==0)
	return 3;
	else
	if(strcmpi(nn,m)==0)
	return 3;
	else
	if(strcmpi(nn,n)==0)
	return 2;
	else
	if(strcmpi(nn,o)==0)
	return 2;
	else
	if(strcmpi(nn,p)==0)
	return 2;
	else
	if(strcmpi(nn,q)==0)
	return 2;
	else
	if(strcmpi(nn,r)==0)
	return 1;
	else
	if(strcmpi(nn,s)==0)
	return 1;
	else
	if(strcmpi(nn,t)==0)
	return 1;
	if(strcmpi(nn,z)==0)
	return 100;
	else
	return 0;
}

void ready()
{
	 cout<<"   _______                          _______ "<<endl;
	 cout<<"  ||-----||  ______   ________     || \\---||  ______      _   ____  _      _"<<endl;
	 cout<<"  ||        ||-----| |---||---|    ||\\ \\  || ||----|     / | ||--\\\\ \\\\    // "<<endl;
	 cout<<"  ||  ____  ||           ||        ||_\\_\\_|| ||         /| | ||   \\\\ \\\\  //  "<<endl;
	 cout<<"  || |---|| ||===|       ||        ||--\\ \\-  ||===|    //| | ||    )) \\\\//   "<<endl;
	 cout<<"  ||     || ||           ||        ||   \\ \\  ||       // | | ||   //   ||    "<<endl;
	 cout<<"  ||_____|| ||_____|     ||        ||    \\|| ||____| |/  | | ||__//    ||    "<<endl;
	 cout<<"   -------   ------      --        --     --  -----  -   ---  ----     --   "<<endl;
	 cout<<"   As soon as you press ENTER, 20 different words will be displayed to you.."<<endl;
	 cout<<"   Try to Memorize the words..."<<endl<<endl;
}

