		cin>>wd;

		if(ck(wd)!=0 && ck(wd)!=100)
		  {
			 kni=0;
			 if(j==1)
			 {
				 for(lp=0;lp<i;lp++)
				  if(strcmpi(re1[lp],wd)==0)
				  kni++;


				 if(kni==0)
				 {

				  if(ck(wd)==1)
				  cout<<"\n\n\n  Thats a right word... You got "<<ck(wd)<<" Point..."<<endl;
				  else
				  cout<<"\n\n\n  Thats a right word... You got "<<ck(wd)<<" Points..."<<endl;
					for(ii=0;ii<50000;ii++)
						 for(jj=0;jj<5000;jj++)
							{
									cc++;
									cc=0;
							}

				  s1+=ck(wd);
				  strcpy(re1[kk1],wd);
				  mm1[kk1]=ck(wd);
				  kk1++;
				 }
				 else
				 {
					cout<<"\n\n   INVALID INPUT: Repetation of words are not allowed"<<endl;
					cout<<"\n                                               YOU JUST LOST A CHANCE..."<<endl;
					cout<<"\n\n\n    PRESS ENTER TO CONTINUE..."<<endl;
					getch();
				 }
			 }
